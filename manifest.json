{
  "name": "sha qiang",
  "version": "0.0.0",
  "description": "A command palette",
  "manifest_version": 2,

  "background": {
    "scripts": [
      "dist/background/index.js"
    ],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": [
        "http://localhost:1234/index.html",
        "http://localhost:1234/"
      ],
      "js": ["dist/content/index.js"]
    }
  ]
}
